---
import Head from "../components/Head.astro";
import Main from "../components/Main.astro";
import Nav from "../components/Nav.astro";
interface Props {
    title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
    <head>
        <Head title={title} />
    </head>
    <body>
        <Nav />
        <Main>
            <slot />
        </Main>
    </body>
</html>
