{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"baseUrl": ".",
		"paths": {
			"@layouts/*": ["src/layouts/*"],
			"@pages/*": ["src/pages/*"],
			"@components/*": ["src/components/*"],
			"@lib/*": ["src/components/lib/*"]
		},
		"allowJs": true
	}
}
